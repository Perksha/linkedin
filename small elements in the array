import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		System.out.println("Enter the size of array");
		int n1=s.nextInt();
		int[] arr= new int[n1];
		for(int i=0;i<n1;i++) {
			arr[i]=s.nextInt();
			
		}
		int small=arr[0];
	    for(int k=1;k<n1;k++){
	        if(arr[k]<small){
	            small=arr[k];
	        }
	    }
		System.out.println("Smallest element in  the array is  "+ small);
}
}
