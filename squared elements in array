import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		System.out.println("Enter the size of array");
		int n=s.nextInt();

		int[] arr= new int[n];
		int j=0;
		for(int i=1;i<=n;i++) {
			arr[j]=i*i;
			j++;
		}
		for(int num:arr) {
			System.out.print(num+" ");
		}
	}

}
